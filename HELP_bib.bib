	Keywords = {galaxies: evolution, galaxies: formation, galaxies: starburst, cosmology: observations, infrared: galaxies},
	Month = apr,
	Pages = {875-882},
	Title = {{Correlations between bright submillimetre sources and low-redshift galaxies}},
